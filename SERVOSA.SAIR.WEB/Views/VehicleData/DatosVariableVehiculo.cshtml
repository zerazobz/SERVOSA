@model SERVOSA.SAIR.WEB.Controllers.TempHeadModel

@using (Html.BeginForm(MVC.VehicleData.DatosVariableVehiculo(), FormMethod.Post, new { id = "postVariableData" }))
{
    @Html.AntiForgeryToken()
    <div class="row">
        <div class="col-md-6">
            @Html.LabelFor(model => model.SomeName)
        </div>
        <div class="col-md-6">
            @Html.DisplayFor(model => model.SomeName)
        </div>
    </div>
    for (int i = 0; i < Model.ChildData.Count; i++)
    {
        <div class="row">
            <div class="col-md-6">
                @Html.TextBoxFor(model => model.ChildData[i].Type)
                @*<input type="text" name="ChildData[@i].Type" value="@Model.ChildData[i].Type" />*@
            </div>
            <div class="col-md-6">
                @Html.TextBoxFor(model => model.ChildData[i].Value)
                @*<input type="text" name="@Model.ChildData[i].Value" value="" />*@
            </div>
        </div>
    }

    <input type="submit" name="submitDatosVariables" value="Guardar" />
}
