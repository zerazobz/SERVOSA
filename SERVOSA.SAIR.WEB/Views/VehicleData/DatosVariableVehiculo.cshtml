@model IList<SERVOSA.SAIR.DATAACCESS.Models.Vehicle.VehicleVariableTableDataModel>

@using (Html.BeginForm(MVC.VehicleData.DatosVariableVehiculo(), FormMethod.Post, new { id = "postVariableData" }))
{
    @Html.AntiForgeryToken()
    foreach (var item in Model)
    {
        <div class="row">
            <div class="col-md-6">
                @Html.LabelFor(f => item.ColumnName)
            </div>
            <div class="col-md-6">
                @Html.TextBoxFor(f => item.TableValue)
            </div>
        </div>
    }

<input type="submit" name="submitDatosVariables" value="Guardar" />
}