@model IList<string>

@{
    ViewBag.Title = "Datos de Vehículos";
}

<input type="hidden" name="CurrentVehicleId" value="@((int)ViewBag.VehicleCode)" />

<div class="row">
    @{ Html.RenderAction(MVC.VehicleData.CabeceraVehiculo((int)ViewBag.VehicleCode)); }
</div>
<hr />
<div class="row">
    <ul class="nav nav-tabs" role="tablist">
        @foreach (var iTableName in Model)
        {
            <li role="presentation">
                <a href="#@iTableName" aria-controls="@iTableName" role="tab" data-toggle="tab" data-vehiclecode="@((int)ViewBag.VehicleCode)">@iTableName</a>
            </li>
        }
    </ul>

    <div class="tab-content">
        @foreach (var iTableName in Model)
        {
            <div role="tabpanel" class="tab-pane" id="@iTableName">
                <div class="innercontenttab" data-containerid="#@iTableName">
                    Content Tab @iTableName
                </div>
                <div class="innerfilestable" data-tablename="@iTableName" data-vehiclecode="@((int)ViewBag.VehicleCode)">
                    <div id="@("filetable" + iTableName)">

                    </div>
                </div>
            </div>
        }
    </div>
</div>


@section scripts {
    <script src="~/Scripts/jquery-ui-1.9.2.js" type="text/javascript"></script>
    <script src="~/Scripts/handlebars-latest.js" type="text/javascript"></script>
    <script src="~/Scripts/ViewController/VehicleViewController.js" type="text/javascript"></script>
    <script src="~/Scripts/ViewController/VehicleDataViewController.js" type="text/javascript"></script>
    <script src="~/Scripts/ViewController/SERVOSAViewController.js" type="text/javascript"></script>
}