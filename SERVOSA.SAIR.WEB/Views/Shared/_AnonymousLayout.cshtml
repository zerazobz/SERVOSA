<!DOCTYPE html>
<html>
<head>
    <meta charset="utf-8" />
    <meta name="viewport" content="width=device-width, initial-scale=1.0">
    <title>@ViewBag.Title</title>
    @Styles.Render("~/Content/css")
    @Scripts.Render("~/bundles/modernizr")
    <script src="~/Scripts/ViewController/DriverSERVOSA.Core.js" type="text/javascript"></script>
    <script src="~/Scripts/ViewController/SERVOSA.Core.js" type="text/javascript"></script>
</head>
<body>

    @*@{ Html.RenderAction(MVC.Shared.Views.Constants._ConstansDrivers); }*@

    @{ Html.RenderPartial(MVC.Shared.Views.Constants._ConstansVehicles); }
    <div class="navbar navbar-inverse navbar-fixed-top">
        <div class="container">
            <div class="navbar-header">
                <button type="button" class="navbar-toggle" data-toggle="collapse" data-target=".navbar-collapse">
                    <span class="icon-bar"></span>
                    <span class="icon-bar"></span>
                    <span class="icon-bar"></span>
                </button>
                @Html.ActionLink(" ", "", "", new { @class = "navbar-brand", href = "javascript:void(0)" })
            </div>
            <div class="navbar-collapse collapse">
                <ul class="nav navbar-nav">
                </ul>
                @Html.Partial("_LoginPartial")
            </div>
        </div>
    </div>
    <div class="container body-content" style="padding-top: 20px;">
        @RenderBody()
        <hr />
        @{ Html.RenderPartial(MVC.Shared.Views._SharedFooter); }
    </div>

    @Scripts.Render("~/bundles/jquery")
    @Scripts.Render("~/bundles/bootstrap")
    @RenderSection("scripts", required: false)
    <script type="text/javascript">
        localStorage.clear();
    </script>
</body>
</html>
